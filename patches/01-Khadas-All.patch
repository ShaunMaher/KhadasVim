+static void aml_sdio_soft_reset(struct amlsd_host *host)
+{
+	struct sdio_irq_config irqc = {0};
+
+	/*soft reset*/
+	irqc.soft_reset = 1;
+	writel(*(u32 *)&irqc, host->base + SDIO_IRQC);
+	udelay(2);
+}
