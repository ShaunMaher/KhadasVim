+
+/ {
+	partitions: partitions{
+        parts = <3>;
+		part-0 = <&logo>;
+		part-1 = <&ramdisk>;
+		part-2 = <&rootfs>;
+
+		logo:logo{
+			pname = "logo";
+			size = <0x0 0x2000000>;
+			mask = <1>;
+		};
+		ramdisk:ramdisk
+		{
+			pname = "ramdisk";
+			size = <0x0 0x2000000>;
+			mask = <1>;
+		};
+		rootfs:rootfs
+		{
+			pname = "rootfs";
+			size = <0xffffffff 0xffffffff>;
+			mask = <4>;
+		};
+	};
+};
