+#ifdef SD_EMMC_REQ_DMA_SGMAP
+static char *aml_sd_emmc_kmap_atomic(
+		struct scatterlist *sg, unsigned long *flags)
+{
+	local_irq_save(*flags);
+	return kmap_atomic(sg_page(sg)) + sg->offset;
+}
