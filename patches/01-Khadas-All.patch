+struct mmc_command aml_sdio_cmd = {
+	.opcode = MMC_STOP_TRANSMISSION,
+	.flags = MMC_RSP_SPI_R1B | MMC_RSP_R1B | MMC_CMD_AC,
+};
+struct mmc_request aml_sdio_stop = {
+	.cmd = &aml_sdio_cmd,
+};
