+static void aml_sdio_send_stop(struct amlsd_host *host)
+{
+	/*Already in mrq_lock*/
+	host->cmd_is_stop = 1;
+	sdio_err_bak = host->mrq->cmd->error;
+	host->mrq->cmd->error = 0;
+	aml_sdio_start_cmd(host->mmc, &aml_sdio_stop);
+}
