+static int is_card_emmc(struct mmc_card *card)
+{
+	struct mmc_host *mmc = card->host;
+
+	/* emmc port, so it must be an eMMC or TSD */
+	if (!strcmp(mmc_hostname(mmc), "mmc1"))//emmc
+		return 1;
+	else
+		return 0;
+	/*return mmc->is_emmc_port;*/
+}
