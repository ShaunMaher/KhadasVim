+		amlsd_get_host_caps(child, pdata);
+		amlsd_get_host_caps2(child, pdata);
+		pdata->port_init = of_amlsd_init;
+		pdata->irq_init = of_amlsd_irq_init;
+		pdata->ro = of_amlsd_ro;
+	}
+	return 0;
+}
