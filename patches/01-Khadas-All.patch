+static bool is_card_last_block(struct amlsd_platform *pdata, u32 lba, u32 cnt)
+{
+	if (!pdata->card_capacity)
+		pdata->card_capacity = mmc_capacity(pdata->mmc->card);
+
+	return (lba + cnt) == pdata->card_capacity;
+}
