+
+	vstat = readl(host->base + SDHC_STAT);
+	sdhc_dbg(AMLSD_DBG_COMMON, "dat[0:3]=%#x\n", stat->dat3_0);
+
+	/* return (stat->dat3_0 == 0); */
+	if (stat->dat3_0 == 0)
+		return 1;
+	else
+		return 0;
+}
