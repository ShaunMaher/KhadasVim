+/*soft reset after errors*/
+void aml_sdhc_host_reset(struct amlsd_host *host)
+{
+	writel(SDHC_SRST_ALL, host->base+SDHC_SRST);
+	udelay(5);
+
+	writel(0, host->base+SDHC_SRST);
+	udelay(10);
+}
