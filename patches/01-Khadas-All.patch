+/*clear fifo after transfer data*/
+void aml_sdhc_clear_fifo(struct amlsd_host *host)
+{
+	struct sdhc_srst srst;
+
+	srst.rxfifo = 1;
+	srst.txfifo = 1;
+	writel(*(u32 *)&srst, host->base+SDHC_SRST);
+	udelay(1);
