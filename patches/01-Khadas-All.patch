+	"invalid",		  /* 0 */
+	"rxfifo full",	  /* 1 */
+	"txfifo empty",	 /* 2 */
+	"rsp CRC",		  /* 3 */
+	"data CRC",		 /* 4 */
+	"rsp timeout",	  /* 5 */
+	"data timeout",	 /* 6 */
+};
